{
  "manifest_version": 3,
  "name": "BeGone",
  "version": "0.2.0",
  "description": "Verwijder VDAB posts van rekruteringskantoren.",
  "permissions": ["storage", "scripting"],
  "host_permissions": ["https://www.vdab.be/*"],
  "content_scripts": [
    {
      "matches": ["https://www.vdab.be/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
